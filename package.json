{
    "name": "enkel-ui",
    "version": "0.0.1-beta.19",
    "description": "A simple and minimal UI component library made for React.",
    "author": "samrith-s",
    "license": "MIT",
    "repository": "samrith-s/enkel",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test:watch": "react-scripts-ts test --env=jsdom",
        "clean": "rimraf dist",
        "build": "yarn clean && rollup -c",
        "start": "yarn clean && rollup -c -w",
        "start:example": "cd example && yarn start",
        "cosmos": "cd example && yarn cosmos",
        "prepare": "yarn run build",
        "predeploy": "cd example && yarn install && yarn run build",
        "deploy": "gh-pages -d example/build"
    },
    "peerDependencies": {
        "polished": "^3.4",
        "react": "^16.8",
        "react-dom": "^16.8",
        "styled-components": "^4"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@svgr/rollup": "^2.4.1",
        "@types/jest": "^23.1.5",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.5",
        "@types/styled-components": "^4.1.18",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "babel-plugin-typescript-to-proptypes": "^1.1.0",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.1.4",
        "eslint": "^6.1.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^1.6.1",
        "gh-pages": "^1.2.0",
        "polished": "^3.4.1",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts-ts": "^2.16.0",
        "rimraf": "^2.6.3",
        "rollup": "^1.17.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.0.1",
        "rollup-plugin-includepaths": "^0.2.3",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.22.1",
        "rollup-plugin-url": "^2.2.2",
        "styled-components": "^4.3.2",
        "stylelint": "^10.1.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-order": "^3.0.1",
        "stylelint-prettier": "^1.1.1",
        "stylelint-processor-styled-components": "^1.8.0",
        "typescript": "^3.5.3"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "prop-types": "^15.7.2"
    }
}
